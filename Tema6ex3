import re

def validate_postal_code(code):
    pattern = r'^[1-9][0-9]{3}[0-9]$'
    if re.match(pattern, code):
        return True
    else:
        return False

# Exemplu de utilizare
code1 = "12345"
code2 = "ABC12"
print(validate_postal_code(code1))  # Output: True
print(validate_postal_code(code2))
