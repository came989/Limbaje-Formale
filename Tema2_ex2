#include <iostream>
#include <string>

using namespace std;

int main() {
    int lungimeMaxima = 3;

    cout << "Iteratia 1:" << endl;
    cout << "aa" << endl;
    cout << "bb" << endl;
    cout << "a" << endl;
    cout << "b" << endl;

    for (int i = 2; i <= lungimeMaxima; ++i) {
        cout << "Iteratia " << i << ":" << endl;
        string s1 = string(i, 'a');
        string s2 = string(i, 'b');
        cout << s1 + s1 << endl;
        cout << s2 + s1 << endl;
        cout << s1 + s2 << endl;
        cout << s2 + s2 << endl;
    }

    return 0;
}
