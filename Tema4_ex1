class FiniteAutomaton:
    def __init__(self):
        self.current_state = 'A'

    def transition(self, symbol):
        if self.current_state == 'A':
            if symbol == '0':
                self.current_state = 'B'
        elif self.current_state == 'B':
            if symbol == '1':
                self.current_state = 'B'
            elif symbol == '0':
                self.current_state = 'A'

    def get_final_state(self):
        return self.current_state


def simulate(input_sequence):
    automaton = FiniteAutomaton()
    for symbol in input_sequence:
        automaton.transition(symbol)
    return automaton.get_final_state()


# Test the automaton with given input sequences
sequences = ["010", "110", "1001"]
for sequence in sequences:
    final_state = simulate(sequence)
    print(f"Final state for sequence '{sequence}': {final_state}")
}
